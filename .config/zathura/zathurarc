### zathura-pywal ###
# Allow recolor
set recolor 'true'

# Don't allow original hue when recoloring
set recolor-keephue 'false'

# Don't keep original image colors while recoloring
set recolor-reverse-video 'false'

# Command line completion entries
set completion-fg '#c5c5c6'
set completion-bg '#1a1a1b'

# Command line completion group elements
set completion-group-fg '#dd834e'
set completion-group-bg '#1a1a1b'

# Current command line completion element
set completion-highlight-fg '#1a1a1b'
set completion-highlight-bg '#c5c5c6'

# Default foreground/background color
set default-bg rgba(26,26,27,0.85)

# Input bar
set inputbar-fg '#c5c5c6'
set inputbar-bg '#1a1a1b'

# Notification
set notification-fg '#c5c5c6'
set notification-bg '#1a1a1b'

# Error notification
set notification-error-fg '#c5c5c6'
set notification-error-bg '#797e85'

# Warning notification
set notification-warning-fg '#c5c5c6'
set notification-warning-bg '#797e85'

# Tab - TODO
# set tabbar-fg
# set tabbar-bg

# Focused tab - TODO
# set tabbar-focus-fg
# set tabbar-focus-bg

# Status bar
set statusbar-fg '#c5c5c6'
set statusbar-bg '#1a1a1b'

# Highlighting parts of the document (e.g. show search results)
set highlight-color '#dd834e'
set highlight-active-color '#dd834e'

# Represent light/dark colors in recoloring mode
set recolor-lightcolor rgba(0,0,0,0)
set recolor-darkcolor '#c5c5c6'

# 'Loading...' text
set render-loading-fg '#c5c5c6'
set render-loading-bg '#1a1a1b'

# Index mode
set index-fg '#c5c5c6'
set index-bg '#1a1a1b'

# Selected element in index mode
set index-active-fg '#1a1a1b'
set index-active-bg '#c5c5c6'