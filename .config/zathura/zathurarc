### zathura-pywal ###
# Allow recolor
set recolor 'true'

# Don't allow original hue when recoloring
set recolor-keephue 'false'

# Don't keep original image colors while recoloring
set recolor-reverse-video 'false'

# Command line completion entries
set completion-fg '#c4c4c4'
set completion-bg '#161615'

# Command line completion group elements
set completion-group-fg '#4a4d50'
set completion-group-bg '#161615'

# Current command line completion element
set completion-highlight-fg '#161615'
set completion-highlight-bg '#c4c4c4'

# Default foreground/background color
set default-bg rgba(22,22,21,0.80)

# Input bar
set inputbar-fg '#c4c4c4'
set inputbar-bg '#161615'

# Notification
set notification-fg '#c4c4c4'
set notification-bg '#161615'

# Error notification
set notification-error-fg '#c4c4c4'
set notification-error-bg '#383a3e'

# Warning notification
set notification-warning-fg '#c4c4c4'
set notification-warning-bg '#383a3e'

# Tab - TODO
# set tabbar-fg
# set tabbar-bg

# Focused tab - TODO
# set tabbar-focus-fg
# set tabbar-focus-bg

# Status bar
set statusbar-fg '#c4c4c4'
set statusbar-bg '#161615'

# Highlighting parts of the document (e.g. show search results)
set highlight-color '#4a4d50'
set highlight-active-color '#4a4d50'

# Represent light/dark colors in recoloring mode
set recolor-lightcolor rgba(0,0,0,0)
set recolor-darkcolor '#c4c4c4'

# 'Loading...' text
set render-loading-fg '#c4c4c4'
set render-loading-bg '#161615'

# Index mode
set index-fg '#c4c4c4'
set index-bg '#161615'

# Selected element in index mode
set index-active-fg '#161615'
set index-active-bg '#c4c4c4'