### zathura-pywal ###
# Allow recolor
set recolor 'true'

# Don't allow original hue when recoloring
set recolor-keephue 'false'

# Don't keep original image colors while recoloring
set recolor-reverse-video 'false'

# Command line completion entries
set completion-fg '#c5c5c4'
set completion-bg '#191716'

# Command line completion group elements
set completion-group-fg '#7f5b3e'
set completion-group-bg '#191716'

# Current command line completion element
set completion-highlight-fg '#191716'
set completion-highlight-bg '#c5c5c4'

# Default foreground/background color
set default-bg rgba(25,23,22,0.80)

# Input bar
set inputbar-fg '#c5c5c4'
set inputbar-bg '#191716'

# Notification
set notification-fg '#c5c5c4'
set notification-bg '#191716'

# Error notification
set notification-error-fg '#c5c5c4'
set notification-error-bg '#683924'

# Warning notification
set notification-warning-fg '#c5c5c4'
set notification-warning-bg '#683924'

# Tab - TODO
# set tabbar-fg
# set tabbar-bg

# Focused tab - TODO
# set tabbar-focus-fg
# set tabbar-focus-bg

# Status bar
set statusbar-fg '#c5c5c4'
set statusbar-bg '#191716'

# Highlighting parts of the document (e.g. show search results)
set highlight-color '#7f5b3e'
set highlight-active-color '#7f5b3e'

# Represent light/dark colors in recoloring mode
set recolor-lightcolor rgba(0,0,0,0)
set recolor-darkcolor '#c5c5c4'

# 'Loading...' text
set render-loading-fg '#c5c5c4'
set render-loading-bg '#191716'

# Index mode
set index-fg '#c5c5c4'
set index-bg '#191716'

# Selected element in index mode
set index-active-fg '#191716'
set index-active-bg '#c5c5c4'