### zathura-pywal ###
# Allow recolor
set recolor 'true'

# Don't allow original hue when recoloring
set recolor-keephue 'false'

# Don't keep original image colors while recoloring
set recolor-reverse-video 'false'

# Command line completion entries
set completion-fg '#c5c4c6'
set completion-bg '#17161b'

# Command line completion group elements
set completion-group-fg '#7d2123'
set completion-group-bg '#17161b'

# Current command line completion element
set completion-highlight-fg '#17161b'
set completion-highlight-bg '#c5c4c6'

# Default foreground/background color
set default-bg rgba(23,22,27,0.80)

# Input bar
set inputbar-fg '#c5c4c6'
set inputbar-bg '#17161b'

# Notification
set notification-fg '#c5c4c6'
set notification-bg '#17161b'

# Error notification
set notification-error-fg '#c5c4c6'
set notification-error-bg '#271a6c'

# Warning notification
set notification-warning-fg '#c5c4c6'
set notification-warning-bg '#271a6c'

# Tab - TODO
# set tabbar-fg
# set tabbar-bg

# Focused tab - TODO
# set tabbar-focus-fg
# set tabbar-focus-bg

# Status bar
set statusbar-fg '#c5c4c6'
set statusbar-bg '#17161b'

# Highlighting parts of the document (e.g. show search results)
set highlight-color '#7d2123'
set highlight-active-color '#7d2123'

# Represent light/dark colors in recoloring mode
set recolor-lightcolor rgba(0,0,0,0)
set recolor-darkcolor '#c5c4c6'

# 'Loading...' text
set render-loading-fg '#c5c4c6'
set render-loading-bg '#17161b'

# Index mode
set index-fg '#c5c4c6'
set index-bg '#17161b'

# Selected element in index mode
set index-active-fg '#17161b'
set index-active-bg '#c5c4c6'