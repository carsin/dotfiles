### zathura-pywal ###
# Allow recolor
set recolor 'true'

# Don't allow original hue when recoloring
set recolor-keephue 'false'

# Don't keep original image colors while recoloring
set recolor-reverse-video 'false'

# Command line completion entries
set completion-fg '#c6c6c6'
set completion-bg '#1d1d1c'

# Command line completion group elements
set completion-group-fg '#857955'
set completion-group-bg '#1d1d1c'

# Current command line completion element
set completion-highlight-fg '#1d1d1c'
set completion-highlight-bg '#c6c6c6'

# Default foreground/background color
set default-bg rgba(29,29,28,0.80)

# Input bar
set inputbar-fg '#c6c6c6'
set inputbar-bg '#1d1d1c'

# Notification
set notification-fg '#c6c6c6'
set notification-bg '#1d1d1c'

# Error notification
set notification-error-fg '#c6c6c6'
set notification-error-bg '#66604d'

# Warning notification
set notification-warning-fg '#c6c6c6'
set notification-warning-bg '#66604d'

# Tab - TODO
# set tabbar-fg
# set tabbar-bg

# Focused tab - TODO
# set tabbar-focus-fg
# set tabbar-focus-bg

# Status bar
set statusbar-fg '#c6c6c6'
set statusbar-bg '#1d1d1c'

# Highlighting parts of the document (e.g. show search results)
set highlight-color '#857955'
set highlight-active-color '#857955'

# Represent light/dark colors in recoloring mode
set recolor-lightcolor rgba(0,0,0,0)
set recolor-darkcolor '#c6c6c6'

# 'Loading...' text
set render-loading-fg '#c6c6c6'
set render-loading-bg '#1d1d1c'

# Index mode
set index-fg '#c6c6c6'
set index-bg '#1d1d1c'

# Selected element in index mode
set index-active-fg '#1d1d1c'
set index-active-bg '#c6c6c6'