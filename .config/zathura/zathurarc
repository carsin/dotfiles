### zathura-pywal ###
# Allow recolor
set recolor 'true'

# Don't allow original hue when recoloring
set recolor-keephue 'false'

# Don't keep original image colors while recoloring
set recolor-reverse-video 'false'

# Command line completion entries
set completion-fg '#c6c5c5'
set completion-bg '#1b1919'

# Command line completion group elements
set completion-group-fg '#59413c'
set completion-group-bg '#1b1919'

# Current command line completion element
set completion-highlight-fg '#1b1919'
set completion-highlight-bg '#c6c5c5'

# Default foreground/background color
set default-bg rgba(27,25,25,0.85)

# Input bar
set inputbar-fg '#c6c5c5'
set inputbar-bg '#1b1919'

# Notification
set notification-fg '#c6c5c5'
set notification-bg '#1b1919'

# Error notification
set notification-error-fg '#c6c5c5'
set notification-error-bg '#4f3f46'

# Warning notification
set notification-warning-fg '#c6c5c5'
set notification-warning-bg '#4f3f46'

# Tab - TODO
# set tabbar-fg
# set tabbar-bg

# Focused tab - TODO
# set tabbar-focus-fg
# set tabbar-focus-bg

# Status bar
set statusbar-fg '#c6c5c5'
set statusbar-bg '#1b1919'

# Highlighting parts of the document (e.g. show search results)
set highlight-color '#59413c'
set highlight-active-color '#59413c'

# Represent light/dark colors in recoloring mode
set recolor-lightcolor rgba(0,0,0,0)
set recolor-darkcolor '#c6c5c5'

# 'Loading...' text
set render-loading-fg '#c6c5c5'
set render-loading-bg '#1b1919'

# Index mode
set index-fg '#c6c5c5'
set index-bg '#1b1919'

# Selected element in index mode
set index-active-fg '#1b1919'
set index-active-bg '#c6c5c5'